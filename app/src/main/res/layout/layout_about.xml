<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="#B9DDAD"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@color/global_color_green_primary"
        android:text=""
        android:textAlignment="center" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/back"
        android:layout_alignBottom="@+id/textView5"
        android:onClick="back"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:fontFamily="monospace"
        android:text="DataBase \nBackend Developer"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_alignTop="@+id/imageView8"
        android:layout_toRightOf="@+id/imageView8"
        android:layout_toEndOf="@+id/imageView8"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView10"
        android:layout_alignLeft="@+id/textView24"
        android:layout_alignStart="@+id/textView24"
        android:layout_marginBottom="17dp"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:fontFamily="monospace"
        android:text="App UI Design \nFront End Developer"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="19dp"
        android:fontFamily="monospace"
        android:text="App Backend Developer \nFunctionality Develpoer"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_alignBottom="@+id/imageView10"
        android:layout_alignLeft="@+id/textView21"
        android:layout_alignStart="@+id/textView21" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/imgbord"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/mani"
        android:layout_marginTop="39dp"
        android:layout_below="@+id/textView24"
        android:onClick="manti"
        android:layout_toRightOf="@+id/imageView"
        android:layout_toEndOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/imgbord"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/me"
        android:onClick="mygit"
        android:layout_marginTop="31dp"
        android:layout_below="@+id/imageView8"
        android:layout_toRightOf="@+id/textView22"
        android:layout_toEndOf="@+id/textView22"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/imgbord"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/z"
        android:onClick="eth"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/imageView9"
        android:layout_alignLeft="@+id/imageView8"
        android:layout_alignStart="@+id/imageView8" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Sophomores @ CSE , NIT Trichy"
        android:textAllCaps="true"
        android:textColor="#00918F"
        android:textStyle="bold"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView22"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView22"
        android:layout_marginBottom="12dp"
        android:text="                      With        by HEAVE"
        android:textColor="#005D82" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="17dp"
        android:layout_height="17dp"
        android:layout_marginEnd="45dp"
        android:layout_marginRight="4dp"
        app:srcCompat="@drawable/pyaar"
        android:layout_alignTop="@+id/textView27"
        android:layout_toLeftOf="@+id/imageView9" />
</RelativeLayout>